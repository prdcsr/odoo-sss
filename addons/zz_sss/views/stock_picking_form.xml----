<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="stock_picking_form">
            <field name="name">stock.picking.sss.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after" >
                    <field name="no_container"/>
                      
                    <field name="is_titipan" attrs="{'invisible': [('picking_type_code', '!=', 'outgoing'),('picking_type_code', 'not in', ['ready','done'])]}"/>           
                </xpath>
            </field>    
        </record>
         <report
            string="Delivery Sticker"
            id="action_delivery_sticker"
            model="stock.picking"
            report_type="qweb-pdf"
            name="zz_sss.report_deliverysticker"
            file="zz_sss.report_deliverysticker"
            print_report_name="'Delivery Sticker - %s - %s' % (object.partner_id.name or '', object.name)"
        />
        <report
            string="Delivery Slip NCR"
            id="action_delivery_slip_ncr"
            model="stock.picking"
            report_type="qweb-pdf"
            name="zz_sss.report_deliveryslipncr"
            file="zz_sss.report_deliveryslipncr"
            print_report_name="'Delivery Slip NCR - %s - %s' % (object.partner_id.name or '', object.name)"
        />
        <report
            string="Delivery Slip NCR Kecil"
            id="action_delivery_slip_ncr_kecil"
            model="stock.picking"
            report_type="qweb-pdf"
            name="zz_sss.report_deliveryslipncrkecil"
            file="zz_sss.report_deliveryslipncrkecil"
            print_report_name="'Delivery Slip NCR Kecil- %s - %s' % (object.partner_id.name or '', object.name)"
        />
        <report
            string="Delivery Slip SP"
            id="action_delivery_slip_sp"
            model="stock.picking"
            report_type="qweb-pdf"
            name="zz_sss.report_deliveryslipsp"
            file="zz_sss.report_deliveryslipsp"
            print_report_name="'Delivery Slip NCR SP- %s - %s' % (object.partner_id.name or '', object.name)"
        />
        <report
            string="Mutasi + SJ"
            id="action_mutasisj"
            model="stock.picking"
            report_type="qweb-pdf"
            name="zz_sss.report_mutasisj"
            file="zz_sss.report_mutasisj"
            print_report_name="'Mutasi SJ- %s - %s' % (object.partner_id.name or '', object.name)"
        />

    </data>
    <template id="report_deliverysticker">
    <t t-call="web.basic_layout">
                <div class="page">
        <t t-foreach="docs" t-as="o">
            <t t-call="zz_sss.delivery_sticker_document" t-lang="o.partner_id.lang"/>
        </t>
        </div>
     </t>   
    </template>

    <template id="delivery_sticker_document">

            <t t-set="o" t-value="o.with_context(lang=o.partner_id.lang)" />
            <div> 
                <t t-set="qr_value" t-value="o.picking_qr_code"/>
                <img t-att-src="'/report/qr/?value=%s&amp;error_correction=%s&amp;version=%s' % (qr_value, 3, 10)" style="width:500;height:500" />
                <p t-field="o.origin"/>
    </div>
        
    </template>  
    
    <template id="report_deliveryslipncr">
    <!-- <t t-call="web.basic_layout"> -->
                <!-- <div class="page"> -->
        <t t-foreach="docs" t-as="o">
            <t t-call="zz_sss.report_delivery_ncr_document" t-lang="o.partner_id.lang"/>
        </t>
        <!-- </div> -->
     <!-- </t>    -->
    </template>

