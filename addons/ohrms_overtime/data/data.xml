         <record id="hr_salary_rule_overtime_outside" model="hr.salary.rule">
            <field name="name">LEMBUR ALL</field>
            <field name="code">OVT_OUT</field>
            <field name="sequence" eval="100"/>
            <field name="category_id" ref="hr_payroll_community.ALW"/>
            <field name="condition_select">python</field>
             <field name="condition_python">result = payslip.input_line_ids</field>
            <field name="amount_select">code</field>
             <field name="amount_python_compute">
total = 0
for line in payslip.input_line_ids:
    if  line.code!="OVTHOUR" and line.code != 'dinasluarkota_25' and line.code != "OT100":
         total += line.amount

result = total
            </field>
        </record>



