            <field name="name">Production:ALSUT (hms_restrict_user)</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <field name="domain_force">[('warehouse_id','in',['ALAM SUTRA']),("name","in",["Production","Waste"])]</field>
            <field name="perm_create" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_unlink" eval="True"/>
            <field name="groups" eval="[(4, ref('hms_restrict_user.group_production_alsut'))]"/>
