<odoo>
    <record id="view_inventory_line_form_uom_helper" model="ir.ui.view">
        <field name="name">stock.inventory.line.uom.helper</field>
        <field name="model">stock.inventory.line</field>
        <field name="inherit_id" ref="stock.stock_inventory_line_tree2"/>
        <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='product_qty']" position="after">
                <field name="helper_uom_id"/>
                <field name="helper_qty"/>
            </xpath>
        </field>
    </record>
</odoo>
