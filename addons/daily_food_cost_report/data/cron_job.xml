<odoo>
    <record id="ir_cron_daily_food_cost_report" model="ir.cron">
        <field name="name">Send Daily Food Cost Report</field>
        <field name="model_id" ref="model_daily_food_cost_report_sender"/>
        <field name="state">code</field>
        <field name="code">model.send_daily_food_cost_report()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
        <field name="nextcall" eval="datetime.now().replace(hour=21, minute=0, second=0, microsecond=0)"/>
    </record>
</odoo>
