<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="action_yearly_gl_wizard" model="ir.actions.act_window">
    <field name="name">GL Daily vs Yearly</field>
    <field name="res_model">yearly.gl.wizard</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
  </record>

  <menuitem id="menu_yearly_gl_report"
            parent="menu_yearly_fx_root"
            name="General Ledger (Daily vs Yearly)"
            action="action_yearly_gl_wizard"
            sequence="20"/>

  <report
      id="report_yearly_gl"
      model="yearly.gl.wizard"
      string="GL Daily vs Yearly"
      report_type="qweb-pdf"
      name="account_yearly_fx.yearly_gl_template"
      file="account_yearly_fx.yearly_gl_template"
      print_report_name="'GL_Daily_vs_Yearly_%s' % (object.date_to)"/>
</odoo>
