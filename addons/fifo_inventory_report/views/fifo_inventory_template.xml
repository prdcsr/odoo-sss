<odoo>
    <template id="report_fifo_inventory_template">
        <t t-call="web.html_container">
            <t t-foreach="docs.report_line_ids" t-as="line">
                <p>
                    <strong><t t-esc="line.product_id.default_code"/> - <t t-esc="line.product_id.name"/></strong><br/>
                    Qty: <t t-esc="line.quantity"/> | Value: <t t-esc="line.value"/> | UoM: <t t-esc="line.product_uom.name"/>
                    <br/>Location: <t t-esc="line.location_id.complete_name"/> | Date: <t t-esc="line.date"/>
                    <t t-if="docs.show_details">
                        <br/>Details: <t t-esc="line.operations"/>
                    </t>
                </p>
            </t>
        </t>
    </template>
</odoo>