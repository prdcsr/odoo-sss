<odoo>
    <record id="ir_cron_kitchen_scheduler" model="ir.cron">
        <field name="name">Return Leftover Stock</field>
        <field name="model_id" ref="stock.model_stock_picking"/>
        <field name="state">code</field>
        <field name="code">model.auto_schedule_return_transfer()</field>
        <field eval="True" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">24</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
<!--        <field name="nextcall" eval="(datetime.datetime.utcnow() + datetime.timedelta(days=1)).replace(hour=7, minute=0, second=0).strftime('%Y-%m-%d %H:%M:%S')"/>-->
        <field eval="False" name="doall" />
    </record>
</odoo>